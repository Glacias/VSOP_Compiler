external strlen(s : string) : int32;

class String { 
    s : string;

    init(theS : string) : string { s <- theS }
    length() : int32 { strlen(s) }
}

class Main extends String {
    main() : int32 {
        let s : string <- init("I am the string") in { print("The string is \"").print(s).print("\"\n") };
        print("Length = ").printInt32(length()).print("\n");
        0
    }
}